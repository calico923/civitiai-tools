# Civitai API settings
CIVITAI_API_KEY=your_api_key_here

# Operation mode
# DOWNLOAD_ENABLED: true/false (default: false)
# false の場合、URL収集のみ実行
DOWNLOAD_ENABLED=false

# Output settings
OUTPUT_FORMAT=csv  # text, csv, json から選択
OUTPUT_DIR=./outputs/urls

# Download settings (DOWNLOAD_ENABLED=true の場合のみ有効)
DOWNLOAD_DIR=./models
MAX_CONCURRENT_DOWNLOADS=1
DOWNLOAD_TIMEOUT=300  # 秒