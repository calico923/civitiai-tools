# ベースモデル別style LoRA検索用の環境変数設定

# Civitai API settings  
CIVITAI_API_KEY=your_api_key_here

# Search settings for style LoRA collection
SEARCH_TYPE=LORA
SEARCH_TAG=style
SEARCH_METHOD=extended
SEARCH_SORT=Highest Rated
MAX_PAGES=5

# Output settings
OUTPUT_FORMAT=csv
OUTPUT_DIR=./outputs/urls

# Base model specific settings
# 個別ベースモデル用: ADDITIONAL_TAG=pony または illustrious または noobai
# ADDITIONAL_TAG=pony